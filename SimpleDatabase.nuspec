			<dependency id="sqlite-net-pcl" version="1.3.1" />
			<dependency id="SQLitePCLRaw.provider.sqlite3.macos" version="1.1.2" />
		  </group>
		</dependencies>
    </metadata>
    <files>
        <file src="SimpleDatabase\bin\Release\SimpleDatabase.dll" target="lib/portable-net45+wp8+wpa81+win8+MonoAndroid10+MonoTouch10+Xamarin.iOS10" />
        <file src="SimpleDatabase\bin\Release\SimpleDatabase.dll" target="lib/netstandard1.1" />
        <file src="SimpleDatabase.iOS\bin\Release\SimpleDatabase.dll" target="lib\Xamarin.iOS10\SimpleDatabase.dll" />
    </files>
</package>